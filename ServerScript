local ReplicatedFirst = game:GetService("ReplicatedFirst")
local ServerScriptService = game:GetService("ServerScriptService")
local ServerStorage = game:GetService("ServerStorage")

-- Cria a pasta no ReplicatedFirst
local newFolder = Instance.new("Folder")
newFolder.Name = "ConteudoMovido"
newFolder.Parent = ReplicatedFirst

-- Função para mover todos os filhos de um serviço
local function moverConteudo(origem)
    for _, obj in ipairs(origem:GetChildren()) do
        obj.Parent = newFolder
    end
end

moverConteudo(ServerScriptService)
moverConteudo(ServerStorage)
